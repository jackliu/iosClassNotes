<?xml version="1.0" ?>
<!--W3C 规定的 XML 的版本 -->
<!-- 这里可以规定 nameSpace ，规定好了整个 xml 中可以使用的标签名称 -->
<!-- 使用 nameSpace 后，你不能随意扩展新的 标签 -->

<!-- 自己扩展任意标签 -->
<iphone_list>
    <iphone>
        <time>2013</time>
        <name>iphone5C</name>
        <price_list>
            <type>
                <size>16G</size>
                <price>4680</price>
            </type>
            <type>
                <size>32G</size>
                <price>5280</price>
            </type>
            <type>
                <size>64G</size>
                <price>6280</price>
            </type>
        </price_list>
    </iphone>
    <iphone>
        <time>2013</time>
        <name>iphone5S</name>
        <price>5680</price>
    </iphone>
</iphone_list>


